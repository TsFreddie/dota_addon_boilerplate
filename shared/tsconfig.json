{
    "compilerOptions": {
        "noImplicitAny" : true,
        "noImplicitThis" : true,
        "alwaysStrict" : true,
        "strictNullChecks" : true,
        "target": "es5",
        "rootDir": ".",
        "sourceMap": false,
    },
    "include": [
        "./**/*",
    ],
    "exclude": [
        "./typings/generated/**/*"
    ],
    "luaTarget": "JIT",
    "luaLibImport": "inline"
}